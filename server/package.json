{
  "name": "server",
  "main": "./build/index.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev ./src/index.ts",
    "dev:watch": "cross-env NODE_ENV=development nodemon ./src/index.ts",
    "lint": "eslint ./src ./documentation --ext .ts",
    "lint:fix": "eslint ./src ./documentation --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc --build",
    "start": "node ./build/index.js",
    "docgen:spec": "ts-node --files ./documentation/docgen/index.ts ./build-docs/spec.yml",
    "docgen:serve": "openapi preview-docs ./build-docs/spec.yml",
    "docgen:build": "redoc-cli build ./build-docs/spec.yml --output ./build-docs/index.html --cdn",
    "docgen:full": "npm run docgen:spec && npm run docgen:build"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "dependencies": {
    "@greencoast/logger": "^1.1.0",
    "ajv": "^8.11.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.1",
    "joi-password": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.1",
    "on-finished": "^2.4.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^7.1.2",
    "redis": "^4.3.1",
    "uniqid": "^5.4.0",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@redocly/cli": "^1.0.0-beta.111",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.18",
    "@types/on-finished": "^2.3.1",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/redis-mock": "^0.17.1",
    "@types/supertest": "^2.0.12",
    "@types/to-json-schema": "^0.2.1",
    "@types/uniqid": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-greencoast": "^0.0.2",
    "felicity": "^6.0.0",
    "jest": "^29.0.3",
    "joi-to-json": "^2.6.1",
    "mockingoose": "^2.15.2",
    "nodemon": "^2.0.20",
    "redis-mock": "^0.56.3",
    "redoc-cli": "^0.13.20",
    "supertest": "^6.2.4",
    "to-json-schema": "^0.2.5",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  }
}
